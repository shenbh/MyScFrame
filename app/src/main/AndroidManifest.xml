<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"          package="com.caihan.myscframe">    <!-- SMS -->    <uses-permission android:name="android.permission.RECEIVE_SMS"/>    <uses-permission android:name="android.permission.READ_SMS"/>    <uses-permission android:name="android.permission.READ_CONTACTS"/>    <!-- ************网络权限 start************ -->    <!-- 用于访问网络，网络定位需要上网 -->    <uses-permission android:name="android.permission.INTERNET"/>    <!-- 用于获取运营商信息，用于支持提供运营商信息相关的接口 -->    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>    <!-- 用于访问wifi网络信息，wifi信息会用于进行网络定位 -->    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>    <!-- ************网络权限 end************ -->    <!-- 获取用户手机的IMEI，用来唯一的标识用户 -->    <!-- 用于读取手机当前的状态,拨打电话和管理通话 -->    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>    <!-- QQ、QQ空间所需权限 -->    <uses-permission android:name="android.permission.GET_TASKS"/>    <!-- ************存储卡权限 start************ -->    <!-- 用于写入缓存数据到扩展存储卡 -->    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>    <!-- ************存储卡权限 end************ -->    <!-- ************拨打电话权限 start************ -->    <uses-permission android:name="android.permission.CALL_PHONE"/>    <!-- ************拨打电话权限 end************ -->    <!-- ************微信支付权限 start************ -->    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS"/>    <!-- ************微信支付权限 end************ -->    <!-- ************ShareSDK新增的权限  start************ -->    <!-- 用于获取wifi的获取权限，wifi信息会用来进行网络定位 -->    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>    <uses-permission android:name="android.permission.MANAGE_ACCOUNTS"/>    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>    <!-- 用于申请获取蓝牙信息进行室内定位 -->    <uses-permission android:name="android.permission.BLUETOOTH"/>    <!-- 用于申请获取蓝牙信息进行室内定位 -->    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>    <!-- ************ShareSDK新增的权限  end************ -->    <!-- ************获取地理位置  start************ -->    <!-- 用于进行网络定位 -->    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>    <!-- 用于访问GPS定位 -->    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>    <!-- ************获取地理位置  end************ -->    <uses-permission android:name="android.permission.RESTART_PACKAGES"/>    <!-- 相机 -->    <uses-permission android:name="android.permission.CAMERA"/>    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>    <!-- ************高德地图新增的权限  start************ -->    <!-- 用于申请调用A-GPS模块 -->    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS"/>    <!-- ************高德地图新增的权限  end************ -->    <!-- ************蒲公英新增的权限  start************ -->    <!-- 允许程序录制音频 -->    <uses-permission android:name="android.permission.RECORD_AUDIO"/>    <!-- 获取logcat日志 -->    <uses-permission android:name="android.permission.READ_LOGS"/>    <!-- ************蒲公英新增的权限  end************ -->    <uses-permission android:name="android.permission.READ_CALENDAR"/>    <uses-permission android:name="android.permission.WRITE_CALENDAR"/>    <!-- 振动权限 -->    <uses-permission android:name="android.permission.VIBRATE"/>    <!-- 闪光权限 -->    <uses-permission android:name="android.permission.FLASHLIGHT"/>    <application        android:name=".MyApp"        android:allowBackup="true"        android:icon="${app_icon}"        android:label="@string/app_name"        android:roundIcon="${app_roundIcon}"        android:supportsRtl="true"        android:resizeableActivity="false"        android:theme="@style/BaseAppTheme">        <!-- 微信分享使用的appid -->        <meta-data            android:name="WEICHAT_APPKEY"            android:value="${WEICHAT_APPKEY}"/>        <meta-data            android:name="WEICHAT_SECRET"            android:value="${WEICHAT_SECRET}"/>        <!-- QQ空间分享使用的appid -->        <meta-data            android:name="QQ_APPKEY"            android:value="${QQ_APPKEY}"/>        <meta-data            android:name="QZ_APPID"            android:value="${QZ_APPID}"/>        <!-- 新浪微博分享使用的appkey -->        <meta-data            android:name="XL_APPKEY"            android:value="${XL_APPKEY}"/>        <meta-data            android:name="XL_SECRET"            android:value="${XL_SECRET}"/>        <!-- OPENIM -->        <meta-data            android:name="OPEN_IM_APPKEY"            android:value="${OPEN_IM_APPKEY}"/>        <meta-data            android:name="OPEN_IM_TARGET_APPKEY"            android:value="${OPEN_IM_TARGET_APPKEY}"/>        <meta-data            android:name="OPEN_SECRET"            android:value="${OPEN_SECRET}"/>        <meta-data            android:name="OPEN_GUIDE_IMAGE_NUM"            android:value="${OPEN_GUIDE_IMAGE_NUM}"/>        <!-- 友盟 -->        <meta-data            android:name="UMENG_APPKEY"            android:value="${UMENG_APPKEY}"/>        <meta-data            android:name="UMENG_MESSAGE_SECRET"            android:value="${UMENG_MESSAGE_SECRET}"/>        <meta-data            android:name="UMENG_CHANNEL_VALUE"            android:value="${UMENG_CHANNEL_VALUE}"/>        <meta-data            android:name="ALIAS_TYPE"            android:value="${ALIAS_TYPE}"/>        <meta-data            android:name="GAODE_KEY"            android:value="${GAODE_KEY}"/>        <meta-data            android:name="BAIDU_MOB_AD_KEY"            android:value="${BAIDU_MOB_AD_KEY}"/>        <activity            android:name=".ui.welcome.WelcomeActivity"            android:theme="@style/BaseAppTheme.ImageTranslucentTheme">            <intent-filter>                <action android:name="android.intent.action.MAIN"/>                <category android:name="android.intent.category.LAUNCHER"/>            </intent-filter>        </activity>        <activity android:name=".ui.home.MainActivity"/>        <activity android:name=".ui.qrcode.ZxingQrcodeActivity">        </activity>        <activity android:name=".ui.qrcode.ZxingQrcodeDemoActivity">        </activity>        <activity android:name=".ui.qrcode.ZxingGenerateActivity">        </activity>        <activity android:name=".ui.multistoreylist.MultiStoreyListActivity">        </activity>        <activity android:name=".ui.immersion.ImmersionActivityDemo">        </activity>        <activity android:name=".ui.immersion.ImmersionFragmentActivity">        </activity>        <activity android:name=".ui.immersion.ImmersionImageActivity">        </activity>        <activity android:name=".ui.immersion.ImmersionFragmentMvpActivity">        </activity>        <activity android:name=".ui.leakcanary.LeakCanaryActivity">        </activity>        <activity android:name=".ui.rxandroid.RxJavaActivity">        </activity>        <activity android:name=".ui.product.ProductActivity">        </activity>        <activity android:name=".ui.multilayerfragment.MultilayerActivity">        </activity>    </application></manifest>